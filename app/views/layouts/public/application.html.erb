                </div>
                <div class="flex-1 flex justify-center">
                  <%= link_to "https://nanai.tools/clusterly-ai?ref=clusterly-ai-homepage", target: "_blank" do %>
                    <%= image_tag "clusterly/embed-badge.svg", class: "", alt: "" %>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
        </div>
        <section class="bg-green-50/60 animate-slidein opacity-0 [--slidein-delay:900ms]" id="features">
          <div class="py-16 px-8 max-w-5xl mx-auto">
            <div class="flex flex-col text-center w-full mb-20">
              <p class="font-medium text-primary mb-8">Features</p>
