                    <%= image_tag "clusterly/people/joseph.webp", class: "h-full w-full object-cover", alt: "Joseph" %>
                  </div>
                </div>
                <div class="flex flex-col justify-center items-center gap-1">
                  <div class="relative inline-flex">
