                    <button class="inline-flex h-12 min-h-12 shrink-0 cursor-pointer items-center justify-center rounded-lg flex-wrap px-4 text-sm bg-indigo-600 text-white font-semibold sm:w-auto hover:bg-indigo-700" type="submit">Unlock Free Articles</button>
                  </form>
                </div>
              </div>
              <div class="flex items-center justify-center sm:gap-8 lg:gap-12 mb-6">
                <div class="flex items-center">
                  <svg class="text-green-600 mr-1 inline-block sm:mr-2" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewbox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                    <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                    <path d="M5 12l5 5l10 -10"></path>
                  </svg>
                  <p class="text-md">No credit card</p>
                </div>
                <div class="flex items-center">
                  <svg class="text-green-600 mr-1 inline-block sm:mr-2" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewbox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                    <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                    <path d="M5 12l5 5l10 -10"></path>
                  </svg>
                  <p class="text-md">6 articles included</p>
                </div>
                <div class="flex items-center">
                  <svg class="text-green-600 mr-1 inline-block sm:mr-2" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewbox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                    <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                    <path d="M5 12l5 5l10 -10"></path>
                  </svg>
                  <p class="text-md">Plagiarism free</p>
                </div>
              </div>
              <div class="flex flex-col md:flex-row justify-center align-center gap-3">
                <div class="-space-x-5 flex overflow-hidden justify-center">
                  <div class="relative inline-flex w-12 h-12 rounded-full overflow-hidden border-4 border-gray-100">
                    <%= image_tag "clusterly/people/peter.webp", class: "h-full w-full object-cover", alt: "Peter" %>
                  </div>
                  <div class="relative inline-flex w-12 h-12 rounded-full overflow-hidden border-4 border-gray-100">
                    <%= image_tag "clusterly/people/harps.webp", class: "h-full w-full object-cover", alt: "Harps" %>
                  </div>
                  <div class="relative inline-flex w-12 h-12 rounded-full overflow-hidden border-4 border-gray-100">
